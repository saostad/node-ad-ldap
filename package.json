{
  "name": "node-ad-ldap",
  "version": "1.3.0",
  "description": "Query Microsoft Active Directory",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/node-ad-ldap.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --check \"src/**/*.ts\" --write",
    "lint": "eslint --ext \".ts,.js\" src --quiet",
    "ts-node": "ts-node src/app.ts",
    "start": "nodemon --watch src -e ts --exec \"npm run ts-node\"",
    "prepublishOnly": "tsc",
    "tsc": "tsc"
  },
  "keywords": [
    "query",
    "ad",
    "active directory",
    "ldap"
  ],
  "author": "Saeid Ostad",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fast-node-logger": "^1.3.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/ldapjs": "^1.0.6",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.15"
  }
}
